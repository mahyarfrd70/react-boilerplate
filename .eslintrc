{
    "settings": {
        "react":{
            "version":"detect"
        }
    },
    "env": {
        "browser": true,
        "jest": true,
        "node": true,
        "es6": true
    },
    "plugins": ["import", "react"],
    "extends": ["eslint:recommended", "plugin:prettier/recommended",  "plugin:react/recommended"],
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true
        },
        "ecmaVersion": 2019,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "warn",
        "no-eval": "error",
        "import/first": "error",
        "no-debugger":"warn"
    }
  }